{
	"name": "code-ecg",
	"displayName": "code-ecg",
	"description": "Code monitor like a ECG",
	"version": "0.0.1",
	"publisher": "team undecided",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openmonitor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openmonitor",
				"title": "Open ECG Monitor"
			}
		],
		"configuration":[
			{
				"title": "Code-ECG",
				"properties": {
					"code-ecg.updateInterval":{
						"type":"integer",
						"default":500,
						"description": "Sets the update interval for the graph in milliseconds."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"@types/vscode": "^1.32.0"
	}
}
